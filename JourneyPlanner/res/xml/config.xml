<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kepler.journeyplanner" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello Cordova</name>
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="EmailComposer">
        <param name="android-package" value="org.apache.cordova.EmailComposer" />
